policy
 lists
  vpn-list Service-VPNs
   vpn 1
   vpn 100
   vpn 101
  !
  site-list Hub
   site-id 100
  !
  site-list Spokes
   site-id 300
   site-id 400
   site-id 500
  !
  prefix-list _AnyIpv4PrefixList
   ip-prefix 0.0.0.0/0 le 32
  !
 !
 control-policy Any-To_Any_VPN
  sequence 1
   match route
    prefix-list _AnyIpv4PrefixList
    vpn-list    Service-VPNs
   !
   action accept
    export-to
     vpn-list Service-VPNs
    !
   !
  !
  default-action accept
 !
!
apply-policy
 site-list Hub
  control-policy Any-To_Any_VPN in
 !
 site-list Spokes
  control-policy Any-To_Any_VPN in
 !
!
